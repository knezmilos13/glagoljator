<?php

namespace BGP\GlagoljicaBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * @ORM\Entity
 * @ORM\Table(name="clanak")
 */
class Clanak {
	
	/**
     * @ORM\Column(type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
	protected $id;
	
	/**
     * @ORM\Column(type="string", length=200)
     */
    protected $naslov;
	
	/**
     * @ORM\Column(type="text")
     */
    protected $tekst;
	
}


?>